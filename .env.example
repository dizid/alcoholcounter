# Supabase Configuration
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key-here
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# Grok AI API
GROK_API_KEY=your-grok-api-key-here

# PayPal Configuration
# For sandbox testing, use sandbox credentials
# For production, use live credentials
PAYPAL_CLIENT_ID=your-paypal-client-id-here
PAYPAL_CLIENT_SECRET=your-paypal-client-secret-here
PAYPAL_PLAN_ID=your-paypal-plan-id-here
PAYPAL_WEBHOOK_ID=your-paypal-webhook-id-here
PAYPAL_SANDBOX=true

# Payment Provider Selection
# Options: 'paypal' or 'stripe' (stripe not yet implemented)
VITE_PAYMENT_PROVIDER=paypal

# Site URL (for PayPal redirects)
URL=http://localhost:8888

# Node Environment
NODE_ENV=development
